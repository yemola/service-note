{
  "expo": {
    "name": "Service Note",
    "slug": "service-note",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/splash-icon-light.png",
    "scheme": "servicenote",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon-light.png",
      "resizeMode": "cover",
      "backgroundColor": "#03BDFF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#03BDFF"
      },
      "package": "com.yemola.servicenote"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash-icon-dark.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ],
      [
        "expo-dev-client"
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to allow you select image if you like to do so."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ab6db3d9-9e98-4524-b5b8-f4ec35720e68"
      }
    }
  }
}